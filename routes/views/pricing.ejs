<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="google-signin-client_id"
        content="326946298299-es2nul9ejjg86ir6d06kauhch4pmjbqv.apps.googleusercontent.com">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Oleo+Script+Swash+Caps:wght@700&display=swap" rel="stylesheet">
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <!-- font awesome  -->
    <script src="https://kit.fontawesome.com/2c582398a5.js" crossorigin="anonymous"></script>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <link rel="stylesheet" href="/stylesheets/index.css">
    <link rel="stylesheet" href="/stylesheets/home.css">

    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
    <title>MultiMedia</title>
</head>

<body>
    <!-- navbar section -->
    <%- include('_navbar') -%>
    <section id="pricing">
        <div id="generic_price_table">
            <section>
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">

                            <div class="price-heading clearfix">
                                <h1>MultiMedia Pricing</h1>
                                <div class="line-mf mb-3"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="generic_content clearfix">
                                <div class="generic_head_price clearfix">
                                    <div class="generic_head_content clearfix">
                                        <div class="head_bg"></div>
                                        <div class="head">
                                            <span>Basic</span>
                                        </div>
                                    </div>
                                    <div class="generic_price_tag clearfix">
                                        <span class="price">
                                            <span class="sign">₹</span>
                                            <span class="currency">49</span>
                                            <span class="cent">.00</span>
                                            <span class="month">/MON</span>
                                        </span>
                                    </div>

                                </div>
                                <div class="generic_feature_list">
                                    <ul>
                                        <li><span>Faster</span> Experience</li>
                                        <li><span>Priority</span> Support</li>
                                        <li><span>Extra</span> Features</li>
                                    </ul>
                                </div>
                                <div class="generic_price_btn clearfix" id="rzp-button1">
                                    <a role="button">Buy now</a>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-4">

                            <div class="generic_content active clearfix">
                                <div class="generic_head_price clearfix">
                                    <div class="generic_head_content clearfix">
                                        <div class="head_bg"></div>
                                        <div class="head">
                                            <span>Standard</span>
                                        </div>
                                    </div>
                                    <div class="generic_price_tag clearfix">
                                        <span class="price">
                                            <span class="sign">₹</span>
                                            <span class="currency">149</span>
                                            <span class="cent">.00</span>
                                            <span class="month">/MON</span>
                                        </span>
                                    </div>
                                </div>
                                <div class="generic_feature_list">
                                    <ul>
                                        <li><span>10k</span> Request limit</li>
                                        <li><span>Beta</span> Version access</li>
                                        <li><span>Priority</span> Support</li>
                                        <li><span>Custom</span> UI</li>
                                        <li><span>24/7</span> Support</li>
                                    </ul>
                                </div>
                                <div class="generic_price_btn clearfix" id="rzp-button2">
                                    <a role="button">Buy now</a>
                                </div>
                            </div>

                        </div>
                        <div class="col-md-4">
                            <div class="generic_content clearfix">
                                <div class="generic_head_price clearfix">
                                    <div class="generic_head_content clearfix">
                                        <div class="head_bg"></div>
                                        <div class="head">
                                            <span>Unlimited</span>
                                        </div>
                                    </div>
                                    <div class="generic_price_tag clearfix">
                                        <span class="price">
                                            <span class="sign">₹</span>
                                            <span class="currency">399</span>
                                            <span class="cent">.00</span>
                                            <span class="month">/MON</span>
                                        </span>
                                    </div>
                                </div>
                                <div class="generic_feature_list">
                                    <ul>
                                        <li><span>Unlimited</span> Request</li>
                                        <li><span>Priority</span> Support</li>
                                        <li><span>Exclusive</span> Badge</li>
                                        <li><span>99%</span> up time</li>
                                        <li><span>24/7</span> Support</li>
                                    </ul>
                                </div>
                                <div class="generic_price_btn clearfix" id="rzp-button3">
                                    <a role="button">Buy now</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </section>
    <script src="https://checkout.razorpay.com/v1/checkout.js"></script>
    <script src="/javascript/payment.js"></script>
</body>

</html>